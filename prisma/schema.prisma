
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
enum   Role {
  writerAndReader 
  Reader  
}
model User{
  id     Int  @id @default(autoincrement())
  name  String
  email  String @unique
  password   String
  role        Role
    createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}